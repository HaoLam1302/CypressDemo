pineline{
    agent any
    tools {nodejs "node"}

    try {
        stages {
        statge('cypress paralell tests')
        parallel {
            stage('tester A'){
                step{
                    sh "npm run cy:parallel"
                }
            }

           stage('tester B'){
                step{
                    sh "npm run cy:parallel"
                }
            }

            stage('tester C'){
                step{
                    sh "npm run cy:parallel"
                }
            }
        }
    }
         
    } finally {
        junit 'build/test-results/test/*.xml'
    }
 }    