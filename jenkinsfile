pipeline{
    agent any
    tools {nodejs "node"}

    stages {
        stage('cypress paralell tests'){
            parallel {
                stage('tester A'){
                    step{
                        bat "npm install cypress --save-dev"
                        bat "npm run cy:parallel"
                    }
                }

              stage('tester B'){
                    steps{
                        bat "npm run cy:parallel"
                    }
                }

                stage('tester C'){
                    steps{
                        bat "npm run cy:parallel"
                    }
                }
            }
        }
}