{
  "baseUrl": "https://all.accor.com/france/index.fr.shtml",
  "ignoreTestFiles": [
    "**/1-getting-started/*.js",
    "**/2-advanced-examples/*.js",
    "**/__snapshots__/*",
    "**/__image_snapshots__/*"
  ],
  "viewportWidth": 1920,
  "viewportHeight": 1024,
  "reporter": "cypress-multi-reporters",
  "reporterOptions": {
    "configFile": "reporter-config.json"
  },
  "env": {
    "host": "rec2",
    "cypress-plugin-snapshots": {
      "imageconfig": {
        "threshold": 0.01
      }
    }
  }
}